{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string"
    },
    "MgmtPublicIPAddressName": {
      "type": "string"
    },
    "PublicIPAddressType": {
      "type": "string"
    },
    "MgmtPublicIPRGName": {
      "type": "string"
    },
    "UntrustPublicIPAddressName": {
      "type": "string"
    },
    "UntrustPublicIPRGName": {
      "type": "string"
    },
    "baseUrl": {
      "type": "string"
    }
  },
  "variables": {
    "SettingUpPublicIPUrl": "[concat(parameters('baseUrl'),'/publicip-untrust-',parameters('UntrustPublicIP'),'.json')]"
  },
  "resources": [
    {
      "name": "UntrustSettingUpPublicIPs",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "properties": { 
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('SettingUpPublicIPUrl')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "location": {
            "value": "[parameters('location')]"
          },
          "publicIPAddressType": {
            "value": "[variables('publicIPAddressType')]"
          },
          "UntrustPublicIPRGName" : {
            "value": "[parameters('UntrustPublicIPRGName')]"
          }
        }
      }
    }
  ],
  "outputs": {
  }
}
